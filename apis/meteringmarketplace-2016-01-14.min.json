{
  "version": "2.0",
  "metadata": {
    "uid": "meteringmarketplace-2016-01-14",
    "apiVersion": "2016-01-14",
    "endpointPrefix": "metering.marketplace",
    "jsonVersion": "1.1",
    "protocol": "json",
    "serviceFullName": "AWSMarketplace Metering",
    "serviceId": "Marketplace Metering",
    "signatureVersion": "v4",
    "signingName": "aws-marketplace",
    "targetPrefix": "AWSMPMeteringService"
  },
  "operations": {
    "BatchMeterUsage": {
      "input": {
        "type": "structure",
        "required": [
          "UsageRecords",
          "ProductCode"
        ],
        "members": {
          "UsageRecords": {
            "shape": "S2"
          },
          "ProductCode": {
            "shape": "S8"
          }
        }
      },
      "output": {
        "type": "structure",
        "members": {
          "Results": {
            "type": "list",
            "member": {
              "type": "structure",
              "members": {
                "UsageRecord": {
                  "shape": "S3"
                },
                "MeteringRecordId": {},
                "Status": {
                  "type": "string",
                  "enum": [
                    "Success",
                    "CustomerNotSubscribed",
                    "DuplicateRecord"
                  ]
                }
              }
            }
          },
          "UnprocessedRecords": {
            "shape": "S2"
          }
        }
      }
    },
    "MeterUsage": {
      "input": {
        "type": "structure",
        "required": [
          "ProductCode",
          "Timestamp",
          "UsageDimension",
          "UsageQuantity",
          "DryRun"
        ],
        "members": {
          "ProductCode": {
            "shape": "S8"
          },
          "Timestamp": {
            "type": "timestamp"
          },
          "UsageDimension": {
            "shape": "S6"
          },
          "UsageQuantity": {
            "shape": "S7"
          },
          "DryRun": {
            "type": "boolean"
          }
        }
      },
      "output": {
        "type": "structure",
        "members": {
          "MeteringRecordId": {}
        }
      }
    },
    "ResolveCustomer": {
      "input": {
        "type": "structure",
        "required": [
          "RegistrationToken"
        ],
        "members": {
          "RegistrationToken": {
            "type": "string",
            "pattern": "\\S+"
          }
        }
      },
      "output": {
        "type": "structure",
        "members": {
          "CustomerIdentifier": {
            "shape": "S5"
          },
          "ProductCode": {
            "shape": "S8"
          }
        }
      }
    }
  },
  "shapes": {
    "S2": {
      "type": "list",
      "member": {
        "shape": "S3"
      },
      "max": 25,
      "min": 0
    },
    "S3": {
      "type": "structure",
      "required": [
        "Timestamp",
        "CustomerIdentifier",
        "Dimension",
        "Quantity"
      ],
      "members": {
        "Timestamp": {
          "type": "timestamp"
        },
        "CustomerIdentifier": {
          "shape": "S5"
        },
        "Dimension": {
          "shape": "S6"
        },
        "Quantity": {
          "shape": "S7"
        }
      }
    },
    "S5": {
      "type": "string",
      "max": 255,
      "min": 1
    },
    "S6": {
      "type": "string",
      "max": 255,
      "min": 1
    },
    "S7": {
      "type": "integer",
      "max": 1000000,
      "min": 0
    },
    "S8": {
      "type": "string",
      "max": 255,
      "min": 1
    }
  }
}