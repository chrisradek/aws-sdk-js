        ],
        "members": {
          "CertificateAuthorityArn": {
            "shape": "Sn"
          },
          "Certificate": {
            "type": "blob",
            "max": 32768,
            "min": 1
          },
          "CertificateChain": {
            "type": "blob",
            "max": 2097152,
            "min": 0
          }
        }
      }
    },
    "IssueCertificate": {
      "input": {
        "type": "structure",
        "required": [
          "CertificateAuthorityArn",
