          },
          "RevocationReason": {
            "type": "string",
            "enum": [
              "UNSPECIFIED",
              "KEY_COMPROMISE",
              "CERTIFICATE_AUTHORITY_COMPROMISE",
              "AFFILIATION_CHANGED",
              "SUPERSEDED",
              "CESSATION_OF_OPERATION",
