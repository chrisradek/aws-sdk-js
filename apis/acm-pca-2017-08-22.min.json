          "Validity"
        ],
        "members": {
          "CertificateAuthorityArn": {
            "shape": "Sn"
          },
          "Csr": {
            "type": "blob",
            "max": 32768,
            "min": 1
          },
          "SigningAlgorithm": {
            "shape": "S4"
          },
          "Validity": {
            "type": "structure",
            "required": [
              "Value",
              "Type"
            ],
            "members": {
              "Value": {
                "type": "long",
                "min": 1
              },
              "Type": {
                "type": "string",
                "enum": [
                  "END_DATE",
                  "ABSOLUTE",
                  "DAYS",
                  "MONTHS",
                  "YEARS"
                ]
              }
            }
          },
          "IdempotencyToken": {
            "shape": "Sl"
          }
        }
      },
      "output": {
        "type": "structure",
        "members": {
          "CertificateArn": {
            "shape": "Sn"
          }
        }
      },
      "idempotent": true
    },
    "ListCertificateAuthorities": {
      "input": {
        "type": "structure",
        "members": {
