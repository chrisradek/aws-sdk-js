          }
        }
      },
      "output": {
        "type": "structure",
        "members": {
          "Tags": {
            "shape": "S1t"
          },
          "NextToken": {
            "shape": "S1n"
          }
        }
      }
    },
    "RestoreCertificateAuthority": {
      "input": {
