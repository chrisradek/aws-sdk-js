{
  "version": "2.0",
  "metadata": {
    "apiVersion": "2017-09-30",
    "endpointPrefix": "kinesisvideo",
    "protocol": "rest-json",
    "serviceAbbreviation": "Kinesis Video Media",
    "serviceFullName": "Amazon Kinesis Video Streams Media",
    "serviceId": "Kinesis Video Media",
    "signatureVersion": "v4",
    "uid": "kinesis-video-media-2017-09-30"
  },
  "operations": {
    "GetMedia": {
      "http": {
        "requestUri": "/getMedia"
      },
      "input": {
        "type": "structure",
        "required": [
          "StartSelector"
        ],
        "members": {
          "StreamName": {
            "type": "string",
            "max": 256,
            "min": 1,
            "pattern": "[a-zA-Z0-9_.-]+"
          },
          "StreamARN": {
            "type": "string",
            "max": 1024,
            "min": 1,
            "pattern": "arn:aws:kinesisvideo:[a-z0-9-]+:[0-9]+:[a-z]+/[a-zA-Z0-9_.-]+/[0-9]+"
          },
          "StartSelector": {
            "type": "structure",
            "required": [
              "StartSelectorType"
            ],
            "members": {
              "StartSelectorType": {
                "type": "string",
                "enum": [
                  "FRAGMENT_NUMBER",
                  "SERVER_TIMESTAMP",
                  "PRODUCER_TIMESTAMP",
                  "NOW",
                  "EARLIEST",
                  "CONTINUATION_TOKEN"
                ]
              },
              "AfterFragmentNumber": {
                "type": "string",
                "max": 128,
                "min": 1,
                "pattern": "^[0-9]+$"
              },
              "StartTimestamp": {
                "type": "timestamp"
              },
              "ContinuationToken": {
                "type": "string",
                "max": 128,
                "min": 1,
                "pattern": "^[a-zA-Z0-9_\\.\\-]+$"
              }
            }
          }
        }
      },
      "output": {
        "type": "structure",
        "members": {
          "ContentType": {
            "location": "header",
            "locationName": "Content-Type",
            "type": "string",
            "max": 128,
            "min": 1,
            "pattern": "^[a-zA-Z0-9_\\.\\-]+$"
          },
          "Payload": {
            "type": "blob",
            "streaming": true
          }
        },
        "payload": "Payload"
      }
    }
  },
  "shapes": {}
}